{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^2.13",
        "doctrine/doctrine-bundle": "^1.11",
        "mongodb/mongodb": "^1.4",
        "nelmio/cors-bundle": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.4",
        "ramsey/uuid": "^3.8",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/mercure-bundle": "^0.3.8",
        "symfony/messenger": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "codeception/codeception": "^4.1.22",
        "codeception/mockery-module": "^0.4.0",
        "codeception/module-asserts": "^1.1",
        "codeception/module-db": "^1.0",
        "codeception/module-mongodb": "^1.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-symfony": "^1.0",
        "codeception/verify": "^1.2",
        "dg/bypass-finals": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Taranto\\ListMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taranto\\ListMaker\\Tests\\": "tests/",
            "Taranto\\ListMaker\\Tests\\Unit\\": "tests/unit/",
            "Taranto\\ListMaker\\Tests\\Integration\\": "tests/integration/",
            "Taranto\\ListMaker\\Tests\\Functional\\": "tests/functional/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
